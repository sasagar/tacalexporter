{
  "name": "TAcalExporter",
  "version": "2.5.0",
  "description": "TechAcademy用メンタリングカレンダーファイル出力アプリ",
  "standard": {
    "globals": [
      "$"
    ]
  },
  "build": {
    "appId": "com.kent-and-co.tacalexporter",
    "mac": {
      "target": "dmg"
    },
    "directories": {
      "output": "../dist"
    }
  },
  "main": "main.js",
  "scripts": {
    "start": "node_modules/.bin/electron .",
    "build": "node build.js",
    "pack:osx": "build --mac --x64",
    "make:win": "electron-packager . TAcalExporter --platform=win32 --arch=x64 --version=1.8.1 --icon=build/icon.ico"
  },
  "repository": {
    "type": "git",
    "url": "https://git.asiist.com/sasagawa/tacalexport.git"
  },
  "author": "SASAGAWA, Kiyoshi",
  "license": "GPL-3.0",
  "dependencies": {
    "about-window": "^1.8.0",
    "bootstrap": "^4.0.0",
    "electron-config": "^1.0.0",
    "electron-json-storage": "^4.0.2",
    "font-awesome": "^4.7.0",
    "gasparesganga-jquery-loading-overlay": "^1.5.4",
    "google-auth-library": "^1.1.0",
    "googleapis": "^25.0.0",
    "jquery": "^3.3.1",
    "jquery-ui": "^1.12.1",
    "path": "^0.12.7",
    "popper": "^1.0.1",
    "popper.js": "^1.13.0"
  },
  "devDependencies": {
    "electron": "^1.8.1",
    "electron-builder": "^19.55.3",
    "electron-packager": "^10.1.2"
  }
}
