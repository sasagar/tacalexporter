{
	"name": "TAcalExporter",
	"version": "3.1.0",
	"description": "TechAcademy用メンタースケジュール登録アプリ",
	"standard": {
		"globals": ["$"]
	},
	"build": {
		"appId": "com.kent-and-co.tacalexporter",
		"mac": {
			"target": "dmg"
		},
		"directories": {
			"output": "../dist"
		}
	},
	"main": "main.js",
	"scripts": {
		"start": "node_modules/.bin/electron .",
		"build": "node build.js",
		"pack:osx": "build --mac --x64",
		"make:win":
			"electron-packager . TAcalExporter --platform=darwin --arch=x64 --version=1.8.4 --icon=build/icon.icns",
		"make:win":
			"electron-packager . TAcalExporter --platform=win32 --arch=x64 --version=1.8.4 --icon=build/icon.ico"
	},
	"repository": {
		"type": "git",
		"url": "https://git.asiist.com/sasagawa/tacalexport.git"
	},
	"author": "SASAGAWA, Kiyoshi",
	"license": "GPL-3.0",
	"dependencies": {
		"about-window": "^1.11.0",
		"bootstrap": "^4.0.0",
		"electron-config": "^1.0.0",
		"electron-gh-releases": "^2.0.4",
		"electron-json-storage": "^4.0.2",
		"font-awesome": "^4.7.0",
		"gasparesganga-jquery-loading-overlay": "^1.6.0",
		"google-auth-library": "^1.3.2",
		"googleapis": "^25.0.0",
		"jquery": "^3.3.1",
		"jquery-ui": "^1.12.1",
		"path": "^0.12.7",
		"popper": "^1.0.1",
		"popper.js": "^1.14.1"
	},
	"devDependencies": {
		"electron": "^1.8.4",
		"electron-builder": "^19.56.2",
		"electron-packager": "^10.1.2"
	}
}
